{
  "name": "ethereumjs-lib",
  "version": "0.5.0",
  "description": "NodeJS and browser JS library for Ethereum",
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ethereum/ethereumjs-lib.git"
  },
  "keywords": [
    "ethereum",
    "bitcoin",
    "crypto",
    "library"
  ],
  "dependencies": {
    "ethashjs": "0.0.2",
    "ethereumjs-account": "0.0.3",
    "ethereumjs-block": "0.0.4",
    "ethereumjs-blockchain": "0.0.0",
    "ethereumjs-tx": "^0.5.9",
    "ethereumjs-util": "^1.3.6",
    "ethereumjs-vm": "^0.1.3",
    "merkle-patricia-tree": "^1.1.1"
  },
  "devDependencies": {
    "browserify": "^8.1.3",
    "eslint": "^0.14.1",
    "ethereum-tests": "git+https://github.com/ethereum/tests#develop",
    "faucet": "0.0.1",
    "level": "^1.1.0",
    "levelup": "^0.19.0",
    "memdown": "^0.10.2",
    "minimist": "^1.1.1",
    "tape": "^4.0.0"
  },
  "testling": {
    "browsers": [
      "ie/9..latest",
      "firefox/15..latest",
      "chrome/22..latest"
    ],
    "files": [
      "test/browserState.js"
    ]
  },
  "scripts": {
    "test": "./bin/tester -a | ./node_modules/faucet/bin/cmd.js",
    "lint": "./script/eslint.sh",
    "compile": "browserify index.js -s Ethereum"
  },
  "author": {
    "name": "Ethereum",
    "url": "http://ethereum.org"
  },
  "contributors": [
    "Vitalik Buterin (https://github.com/vbuterin)",
    "Martin Becze <wanderer.github.io> (github.com/wanderer)",
    "Ethers (github.com/ethers)",
    "Aaron Kumavis <http://aaron.kumavis.me/> (https://github.com/kumavis)"
  ]
}

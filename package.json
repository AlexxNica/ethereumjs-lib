{
  "name": "ethereumjs-lib",
  "version": "0.1.8",
  "description": "NodeJS and browser JS library for Ethereum",
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ethereum/ethereumjs-lib.git"
  },
  "keywords": [
    "ethereum",
    "bitcoin",
    "crypto",
    "library"
  ],
  "dependencies": {
    "JSONStream": "^0.10.0",
    "async": "^0.2.10",
    "bigi": "^1.4.0",
    "bignum": "^0.9.1",
    "bignum-browserify": "git+https://github.com/wanderer/bignum-browserify",
    "crypto-js": "^3.1.2-5",
    "ecdsa": "^0.5.3",
    "ecurve": "^0.6.0",
    "ethereumjs-p2p": "git+https://github.com/ethereum/ethereumjs-p2p",
    "ethereumjs-util": "git+https://github.com/ethereum/ethereumjs-util",
    "levelup": "^0.19.0",
    "memdown": "^0.10.2",
    "merkle-patricia-tree": "^0.1.23",
    "minimist": "^1.1.0",
    "mocha": "^2.1.0",
    "rlp": "0.0.12",
    "secp256k1": "0.0.13",
    "sha3": "^1.1.0",
    "uglifyjs": "^2.3.6",
    "underscore": "^1.6.0"
  },
  "devDependencies": {
    "JSONStream": "^0.10.0",
    "eslint": "^0.8.2",
    "ethereum-tests": "0.0.3",
    "levelup": "^0.19.0",
    "memdown": "^0.10.2",
    "minimist": "^1.1.0",
    "tape": "^3.5.0"
  },
  "testling": {
    "browsers": [
      "ie/9..latest",
      "firefox/15..latest",
      "chrome/22..latest"
    ],
    "harness": "mocha-bdd",
    "files": [
      "test/stateTests.js"
    ]
  },
  "scripts": {
    "test": "./script/eslint.sh && ./bin/tester -a",
    "compile": "browserify index.js -s Ethereum"
  },
  "author": {
    "name": "Ethereum",
    "url": "http://ethereum.org"
  },
  "contributors": [
    "Vitalik Buterin (ttps://github.com/vbuterin)",
    "Martin Becze <wanderer.github.io> (github.com/wanderer)",
    "Ethers (github.com/ethers)"
  ],
  "browser": {
    "transform": [
      "brfs"
    ],
    "ecdsa-native": false,
    "./lib/ecdsaOps.js": "./browser/ecdsaOps.js",
    "bignum": "bignum-browserify",
    "sha3": "./browser/sha3.js"
  }
}
